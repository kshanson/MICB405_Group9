### STAR ###

# control_200_2 aligned

STAR \
--genomeDir /projects/micb405/finalprojects2024/group09/reference/indexed_reference \
--readFilesIn /projects/micb405/finalprojects2024/group09/SRP075510/SRR3567676/SRR3567676_1.fastq.gz /projects/micb405/finalprojects2024/group09/SRP075510/SRR3567676/SRR3567676_2.fastq.gz \
--readFilesCommand zcat \
--outFileNamePrefix control_200_2 \
--outSAMtype BAM SortedByCoordinate \
--quantMode GeneCounts \
--runThreadN 8 \
--alignIntronMax 1500 &

